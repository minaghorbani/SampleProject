}
@section Tags{


}
@section PageHeader{
    <div class="" style="padding-top: 140px; padding-bottom: 120px; background-repeat: no-repeat; background-position: center top; background-image: url('/lib/pic/home_recipes2_slider_bg.jpg');">
        <div class="row" style="margin:0px;">
            <div class="col-6"></div>
            <div class="col-6" style="text-align:right;">
                <h6 class="text-success font-weight-bold">
                    به کوک استپس خوش آمدید
                </h6>
                <h2 class="font-weight-bold text-lg-right" style="text-align:right; padding-top:10px; font-size:30px; font-weight:bold">آشپزی آسان<br> گام به گام با Cook Steps</h2>
                <br />
                <p> همراه ما باشید تا با هم از آشپزی لذت ببریم</p>
                <br />
